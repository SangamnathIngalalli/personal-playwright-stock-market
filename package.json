    "test": "echo \"Error: no test specified\" && exit 1",
    "test:scrap": "npx playwright test scrap-52weeks-high-stock.spec.ts",
    "test:track": "npx playwright test 52weeks-track-stocks.spec.ts",
    "test:update": "npx playwright test update-prices-track-stocks.spec.ts",
    "test:all": "npm run test:scrap && npm run test:track && npm run test:update",
    "test:sequential": "npx playwright test --grep \"Extract company names|Track 52-week high stocks|Update tracking file\"",
